<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.activity"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />
    
    <uses-permission android:name="android.permission.INTERNET" />
    
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.test.activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- android:theme="@android:style/Theme.Dialog" -->
        <activity android:name="com.test.activity.GuideActivity" android:label="@string/app_name" ></activity>
        <activity android:name="com.test.activity.RecoActivity" android:label="@string/app_name" ></activity>
        <activity android:name="com.test.activity.WebViewActivity" android:label="@string/app_name" ></activity>
    </application>

</manifest>
